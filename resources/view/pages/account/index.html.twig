																		<tr id="CharacterRow_{{ count }}" onmouseover="InRowWithOverEffect('CharacterRow_{{ count }}', '#ffedd1');" onmouseout="OutRowWithOverEffect('CharacterRow_{{ count }}', '{{ getStyle(i) }}');" onclick="FocusCharacter({{ count }}, '{{ account.player.name }}', 9);" class="CharacterList {{ getStyleCss(i) }}" style="font-weight: bold;">
																			<td style="text-align: center; padding:2px; padding-top: 4px;">{{ count }}.</td>
																			<td><img src="{{ account.player.outfit.image_url }}" alt="Outfit" title="Outfit" height="55" width="55" style="left: -10px; bottom: 10px; position: inherit; border-radius: 0;"></td>
																			<td id="CharacterCell2_{{ count }}">
																				<span style="white-space: nowrap; vertical-align: middle; line-height: 12px;">
																					<span id="CharacterNameOf_{{ count }}" {% if account.player.deletion == true %}style="font-size:13pt; color: red"{% else %}style="font-size:13pt;"{% endif %}>{{ account.player.name }}
