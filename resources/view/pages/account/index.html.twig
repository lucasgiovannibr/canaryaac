                                                                                        <img id="VisibleState" src="{{IMAGE_PATH}}/global/content/icon-status-hidden.png" alt="hidden" title="hidden">
																						{% endif %}
																						{% if player.online == true %}
                                                                                        <img id="VisibleState" src="{{IMAGE_PATH}}/on.gif" alt="Online" title="Online">
																						{% else %}
																						<img id="VisibleState" src="{{IMAGE_PATH}}/off.gif" alt="Offline" title="Offline">
																						{% endif %}
																						{% if player.skull is not empty %}
																						<img src="{{IMAGE_PATH}}/{{ player.skull }}" alt="Skull" title="Skull">
																						{% endif %}
                                                                                    </td>
